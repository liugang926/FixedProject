{"ast":null,"code":"import path from 'path-browserify';\nimport { isExternal } from '@/utils/validate';\nexport default {\n  name: 'SidebarItem',\n  props: {\n    item: {\n      type: Object,\n      required: true\n    },\n    basePath: {\n      type: String,\n      \"default\": ''\n    },\n    isNest: {\n      type: Boolean,\n      \"default\": false\n    }\n  },\n  methods: {\n    hasChildren: function hasChildren(item) {\n      return item.children && item.children.length > 0;\n    },\n    showItem: function showItem(item) {\n      return true;\n    },\n    resolvePath: function resolvePath(routePath) {\n      if (isExternal(routePath)) {\n        return routePath;\n      }\n      if (isExternal(this.basePath)) {\n        return this.basePath;\n      }\n      return path.resolve(this.basePath, routePath);\n    }\n  }\n};","map":{"version":3,"names":["path","isExternal","name","props","item","type","Object","required","basePath","String","isNest","Boolean","methods","hasChildren","children","length","showItem","resolvePath","routePath","resolve"],"sources":["C:\\Users\\ND\\Desktop\\Fixed_Project\\frontend\\src\\components\\layout\\SidebarItem.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"!item.hidden\">\r\n    <!-- 没有子路由的情况 -->\r\n    <template v-if=\"!hasChildren(item)\">\r\n      <el-menu-item \r\n        v-if=\"showItem(item)\"\r\n        :index=\"resolvePath(item.path)\"\r\n        :class=\"{'submenu-title-noDropdown': !isNest}\"\r\n      >\r\n        <el-icon v-if=\"item.meta && item.meta.icon\">\r\n          <component :is=\"item.meta.icon\" />\r\n        </el-icon>\r\n        <template #title>\r\n          <span v-if=\"item.meta\">{{ item.meta.title }}</span>\r\n        </template>\r\n      </el-menu-item>\r\n    </template>\r\n\r\n    <!-- 有子路由的情况 -->\r\n    <el-sub-menu \r\n      v-else \r\n      :index=\"resolvePath(item.path)\"\r\n      popper-append-to-body\r\n    >\r\n      <template #title>\r\n        <el-icon v-if=\"item.meta && item.meta.icon\">\r\n          <component :is=\"item.meta.icon\" />\r\n        </el-icon>\r\n        <span v-if=\"item.meta\">{{ item.meta.title }}</span>\r\n      </template>\r\n      <sidebar-item\r\n        v-for=\"child in item.children\"\r\n        :key=\"child.path\"\r\n        :item=\"child\"\r\n        :base-path=\"resolvePath(item.path)\"\r\n        :is-nest=\"true\"\r\n      />\r\n    </el-sub-menu>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport path from 'path-browserify'\r\nimport { isExternal } from '@/utils/validate'\r\n\r\nexport default {\r\n  name: 'SidebarItem',\r\n  props: {\r\n    item: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    basePath: {\r\n      type: String,\r\n      default: ''\r\n    },\r\n    isNest: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  },\r\n  methods: {\r\n    hasChildren(item) {\r\n      return item.children && item.children.length > 0\r\n    },\r\n    showItem(item) {\r\n      return true\r\n    },\r\n    resolvePath(routePath) {\r\n      if (isExternal(routePath)) {\r\n        return routePath\r\n      }\r\n      if (isExternal(this.basePath)) {\r\n        return this.basePath\r\n      }\r\n      return path.resolve(this.basePath, routePath)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.el-menu-item, .el-sub-menu {\r\n  .el-icon {\r\n    margin-right: 16px;\r\n    vertical-align: middle;\r\n    width: 24px;\r\n    text-align: center;\r\n  }\r\n  \r\n  .el-sub-menu__icon-arrow {\r\n    margin-right: 0;\r\n  }\r\n}\r\n</style> "],"mappings":"AA0CA,OAAOA,IAAG,MAAO,iBAAgB;AACjC,SAASC,UAAS,QAAS,kBAAiB;AAE5C,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACLC,IAAI,EAAE;MACJC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ,CAAC;IACDC,QAAQ,EAAE;MACRH,IAAI,EAAEI,MAAM;MACZ,WAAS;IACX,CAAC;IACDC,MAAM,EAAE;MACNL,IAAI,EAAEM,OAAO;MACb,WAAS;IACX;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,WAAW,WAAXA,WAAWA,CAACT,IAAI,EAAE;MAChB,OAAOA,IAAI,CAACU,QAAO,IAAKV,IAAI,CAACU,QAAQ,CAACC,MAAK,GAAI;IACjD,CAAC;IACDC,QA