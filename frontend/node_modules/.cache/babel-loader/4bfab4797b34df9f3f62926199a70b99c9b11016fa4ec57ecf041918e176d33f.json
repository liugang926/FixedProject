{"ast":null,"code":"import { defineComponent, computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRouter } from 'vue-router';\nimport { Fold, Expand, CaretBottom } from '@element-plus/icons-vue';\nimport Breadcrumb from './Breadcrumb.vue';\nexport default defineComponent({\n  name: 'Navbar',\n  components: {\n    Breadcrumb,\n    Fold,\n    Expand,\n    CaretBottom\n  },\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const userName = computed(() => store.state.user.name || '用户');\n    const userAvatar = computed(() => store.state.user.avatar);\n    const userInitials = computed(() => userName.value.charAt(0).toUpperCase());\n    const collapsed = computed(() => store.state.app.sidebarCollapsed);\n    const toggleSidebar = () => {\n      store.commit('app/TOGGLE_SIDEBAR');\n    };\n    const handleProfile = () => {\n      router.push('/profile');\n    };\n    const handleLogout = async () => {\n      await store.dispatch('user/logout');\n      router.push('/login');\n    };\n    return {\n      userName,\n      userAvatar,\n      userInitials,\n      collapsed,\n      toggleSidebar,\n      handleProfile,\n      handleLogout\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","computed","useStore","useRouter","Fold","Expand","CaretBottom","Breadcrumb","name","components","setup","store","router","userName","state","user","userAvatar","avatar","userInitials","value","charAt","toUpperCase","collapsed","app","sidebarCollapsed","toggleSidebar","commit","handleProfile","push","handleLogout","dispatch"],"sources":["C:\\Users\\ND\\Desktop\\Fixed_Project\\frontend\\src\\components\\layout\\Navbar.vue"],"sourcesContent":["<template>\r\n  <div class=\"navbar\">\r\n    <div class=\"left-menu\">\r\n      <el-button type=\"text\" @click=\"toggleSidebar\">\r\n        <el-icon><Fold v-if=\"!collapsed\" /><Expand v-else /></el-icon>\r\n      </el-button>\r\n      <breadcrumb class=\"breadcrumb-container\" />\r\n    </div>\r\n    \r\n    <div class=\"right-menu\">\r\n      <el-dropdown trigger=\"click\">\r\n        <div class=\"avatar-wrapper\">\r\n          <el-avatar :size=\"30\" :src=\"userAvatar\">{{ userInitials }}</el-avatar>\r\n          <span class=\"user-name\">{{ userName }}</span>\r\n          <el-icon><CaretBottom /></el-icon>\r\n        </div>\r\n        <template #dropdown>\r\n          <el-dropdown-menu>\r\n            <el-dropdown-item @click=\"handleProfile\">个人信息</el-dropdown-item>\r\n            <el-dropdown-item divided @click=\"handleLogout\">退出登录</el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </template>\r\n      </el-dropdown>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { defineComponent, computed } from 'vue'\r\nimport { useStore } from 'vuex'\r\nimport { useRouter } from 'vue-router'\r\nimport { Fold, Expand, CaretBottom } from '@element-plus/icons-vue'\r\nimport Breadcrumb from './Breadcrumb.vue'\r\n\r\nexport default defineComponent({\r\n  name: 'Navbar',\r\n  components: {\r\n    Breadcrumb,\r\n    Fold,\r\n    Expand,\r\n    CaretBottom\r\n  },\r\n  setup() {\r\n    const store = useStore()\r\n    const router = useRouter()\r\n\r\n    const userName = computed(() => store.state.user.name || '用户')\r\n    const userAvatar = computed(() => store.state.user.avatar)\r\n    const userInitials = computed(() => userName.value.charAt(0).toUpperCase())\r\n    const collapsed = computed(() => store.state.app.sidebarCollapsed)\r\n\r\n    const toggleSidebar = () => {\r\n      store.commit('app/TOGGLE_SIDEBAR')\r\n    }\r\n\r\n    const handleProfile = () => {\r\n      router.push('/profile')\r\n    }\r\n\r\n    const handleLogout = async () => {\r\n      await store.dispatch('user/logout')\r\n      router.push('/login')\r\n    }\r\n\r\n    return {\r\n      userName,\r\n      userAvatar,\r\n      userInitials,\r\n      collapsed,\r\n      toggleSidebar,\r\n      handleProfile,\r\n      handleLogout\r\n    }\r\n  }\r\n})\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.navbar {\r\n  height: 60px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n\r\n  .left-menu {\r\n    display: flex;\r\n    align-items: center;\r\n    \r\n    .breadcrumb-container {\r\n      margin-left: 15px;\r\n    }\r\n  }\r\n\r\n  .right-menu {\r\n    margin-right: 20px;\r\n\r\n    .avatar-wrapper {\r\n      display: flex;\r\n      align-items: center;\r\n      cursor: pointer;\r\n      \r\n      .user-name {\r\n        margin: 0 8px;\r\n        color: #606266;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style> "],"mappings":"AA4BA,SAASA,eAAe,EAAEC,QAAO,QAAS,KAAI;AAC9C,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,SAAQ,QAAS,YAAW;AACrC,SAASC,IAAI,EAAEC,MAAM,EAAEC,WAAU,QAAS,yBAAwB;AAClE,OAAOC,UAAS,MAAO,kBAAiB;AAExC,eAAeP,eAAe,CAAC;EAC7BQ,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACVF,UAAU;IACVH,IAAI;IACJC,MAAM;IACNC;EACF,CAAC;EACDI,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIT,QAAQ,CAAC;IACvB,MAAMU,MAAK,GAAIT,SAAS,CAAC;IAEzB,MAAMU,QAAO,GAAIZ,QAAQ,CAAC,MAAMU,KAAK,CAACG,KAAK,CAACC,IAAI,CAACP,IAAG,IAAK,IAAI;IAC7D,MAAMQ,UAAS,GAAIf,QAAQ,CAAC,MAAMU,KAAK,CAACG,KAAK,CAACC,IAAI,CAACE,MAAM;IACzD,MAAMC,YAAW,GAAIjB,QAAQ,CAAC,MAAMY,QAAQ,CAACM,KAAK,CAACC,MAAM,CAAC,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC;IAC1E,MAAMC,SAAQ,GAAIrB,QAAQ,CAAC,MAAMU,KAAK,CAACG,KAAK,CAACS,GAAG,CAACC,gBAAgB;IAEjE,MAAMC,aAAY,GAAIA,CAAA,KAAM;MAC1Bd,KAAK,CAACe,MAAM,CAAC,oBAAoB;IACnC;IAEA,MAAMC,aAAY,GAAIA,CAAA,KAAM;MAC1Bf,MAAM,CAACgB,IAAI,CAAC,UAAU;IACxB;IAEA,MAAMC,YAAW,GAAI,MAAAA,CAAA,KAAY;MAC/B,MAAMlB,KAAK,CAACmB,QAAQ,CAAC,aAAa;MAClClB,MAAM,CAACgB,IAAI,CAAC,QAAQ;IACtB;IAEA,OAAO;MACLf,QAAQ;MACRG,UAAU;MACVE,YAAY;MACZI,SAAS;MACTG,aAAa;MACbE,aAAa;MACbE;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}